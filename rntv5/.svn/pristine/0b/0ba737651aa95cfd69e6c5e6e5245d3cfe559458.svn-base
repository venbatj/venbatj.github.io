    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="order-rfid-table">
                    <thead>
                        <tr>
                            <th width="70px">S.No</th>
                            <th>RFID Supply Vendor</th>
                            <th>Qty</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="Data in OrderRFIDData">
                            <td>{{$index + 1}}</td>
                            <td>{{Data.RFIDSupplierName}}</td>
                            <td>{{Data.QTY}}</td>
                            <td><a title="Remove" ng-click="removeData($index, Data.ID)" class="delete-icon fa fa-trash font-red-thunderbird"></a>
                                <a title="Edit" ng-click="editData($index, Data.ID)" class="edit-icon fa fa-edit font-green-jungle"></a></td>
                        </tr>
                    </tbody>
                </table>
                <div class="col-xs-12">
                    <div class="form-group">
                        <label>&nbsp;</label>
                        <div class="action-group">
                            <button type="button" ng-click="saveOrderRFID(PrntFrm.$valid)" class="btn blue fa fa-send-o" ng-disabled="PrntFrm.$invalid">Send</button>
                            <button type="button" ng-click="cancel()" class="btn blue fa fa-close">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>	
        </div>
    </div>
</div>
<div class="modal fade in" id="saveinventory"  data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h3><span class="modal-title custom-font-color">RNT - Order RFID</span></h3>
    </div>
    <div class="modal-header">
        <p><h4> &nbsp;PO Request Sent Successfully !</h4></p>
    </div>
</div>