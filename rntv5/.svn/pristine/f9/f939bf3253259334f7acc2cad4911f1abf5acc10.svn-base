<form name="FormContractInfo" class="horizontal-form" method="post">
    <div class="portlet-body">
        <div class="form-body">
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.ContractSignedDate.$invalid && FormContractInfo.ContractSignedDate.$touched}">
                        <label class="fld-required">Contract Signed Date</label>
                        <div class="input-group">
                            <div class="input-group date">
                                <input type="text" class="form-control" name="ContractSignedDate" id="ContractSignedDate" ng-init="InputMask()"
                                    ng-disabled="disable" ng-model="ContractInfo.ContractSignedDate" required> 
                                <span class="input-group-addon" id="ContractSignedDate" >
                                    <span class="fa fa-calendar" id="calendar"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.ContractStartDate.$invalid && FormContractInfo.ContractStartDate.$touched}">
                        <label class="fld-required">Contract Start Date</label>
                        <div class="input-group date">
                            <input type="text" class="form-control" id="contract-start-date" name="ContractStartDate"
                             ng-disabled="disable" ng-model="ContractInfo.ContractStartDate" ng-init="InputMask()"  required> 
                            <span class="input-group-addon" id="contract-start-date" >
                                <span class="fa fa-calendar" id="start-date"></span>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.ContractEndDate.$invalid && FormContractInfo.ContractEndDate.$touched || ContractError}">
                        <label class="fld-required">Contract End Date</label>
                        <div class="input-group">
                            <input type="text" ng-change="endDate()" class="form-control" id="contract-end-date" name="ContractEndDate"
                             ng-disabled="disable" ng-model="ContractInfo.ContractEndDate" ng-init="InputMask()" ng-blur="reminder()" required> 
                            <span class="input-group-addon" id="contract-end-date" >
                                <span class="fa fa-calendar" id="end-date"></span>
                            </span>
                        </div>
                        <p ng-show="ContractError" class="help-block">This End Date must be Greater than Start Date</p>                         
                        
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.InvoicingFrequencyID.$invalid && FormContractInfo.InvoicingFrequencyID.$touched}">
                        <label class="fld-required">Invoicing Frequency</label>
                        <select id="invoicing-frequency" name="InvoicingFrequencyID" ng-disabled="disable" ng-model="ContractInfo.InvoicingFrequencyID" class="form-control select2" required> 
                            <option value="{{If.ID}}" ng-repeat="If in InvoicingFrequencyList">{{If.InvoicingFrequency}}</option>
                        </select>     
                        <p ng-show="FormContractInfo.InvoicingFrequencyID.$invalid && FormContractInfo.InvoicingFrequencyID.$touched" class="help-block">This field is required.</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.Advance.$invalid && FormContractInfo.Advance.$touched}">
                        <label class="fld-required">Advance</label>
                        <div class="input-group">
                            <select id="advance" name="Advance" class="form-control select2" required
                            ng-disabled="disable" ng-model="ContractInfo.Advance" ng-change="onAdvanceChange(A.Advance)">
                                <option value="{{A.ID}}" ng-repeat="A in AdvanceList">{{A.Advance}}</option>
                            </select>
                            <p ng-show="FormContractInfo.Advance.$invalid && FormContractInfo.Advance.$touched" class="help-block">This field is required.</p>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.PaymentTerms.$invalid && FormContractInfo.PaymentTerms.$touched}">
                        <label class="fld-required">Payment Terms</label>
                        <select id="payment-terms" name="PaymentTerms" ng-disabled="disable" ng-model="ContractInfo.PaymentTerms"class="form-control select2" required> 
                            <option value="7">7</option>
                            <option value="14">14</option>
                            <option value="21">21</option>
                            <option value="28">28</option>
                            <option value="35">35</option>
                            <option value="42">42</option>
                            <option value="49">49</option>
                            <option value="56">56</option>
                            <option value="63">63</option>
                            <option value="70">70</option>
                            <option value="77">77</option>
                            <option value="84">84</option>
                            <option value="91">91</option>
                        </select>
                        <p ng-show="FormContractInfo.PaymentTerms.$invalid && FormContractInfo.PaymentTerms.$touched" class="help-block">This field is required.</p>     
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.FreightTerms.$invalid && FormContractInfo.FreightTerms.$touched}">
                        <label class="fld-required">Freight Terms</label>
                        <div class="input-group">
                            <input type="text" class="form-control" NAME="FreightTerms" id="FreightTerms" ng-disabled="disable" ng-model="ContractInfo.FreightTerms" required> 
                        <p ng-show="FormContractInfo.FreightTerms.$invalid && FormContractInfo.FreightTerms.$touched" class="help-block">This field is required.</p>     
                    </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.ReturnPolicy.$invalid && FormContractInfo.ReturnPolicy.$touched}">
                        <label class="fld-required">Return Policy</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="ReturnPolicy" ng-disabled="disable" ng-model="ContractInfo.ReturnPolicy" required> 
                        <p ng-show="FormContractInfo.ReturnPolicy.$invalid && FormContractInfo.ReturnPolicy.$touched" class="help-block">This field is required.</p>     
                    </div>
                    </div>
                </div>
                </div>
                <div class="row">
                <!-- <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.LeadTime.$invalid && FormContractInfo.LeadTime.$touched}">
                        <label class="fld-required" for="LeadTime">Lead Time</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="LeadTime" id="LeadTime" ng-disabled="disable" ng-model="ContractInfo.LeadTime" required> 
                            <p ng-show="FormContractInfo.LeadTime.$invalid && FormContractInfo.LeadTime.$touched" class="help-block">This field is required.</p> 
                        </div>
                    </div>
                </div> -->
            <!-- </div>
            <div class="row"> -->
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.LeadTime.$invalid && FormContractInfo.LeadTime.$touched}">
                        <label class="fld-required" for="LeadTime">Lead Time</label>
                        <select id="lead-time" name="LeadTime" ng-disabled="disable" ng-model="ContractInfo.LeadTime"class="form-control select2" required> 
                            <option value="7">7</option>
                            <option value="14">14</option>
                            <option value="21">21</option>
                            <option value="28">28</option>
                            <option value="35">35</option>
                            <option value="42">42</option>
                            <option value="49">49</option>
                            <option value="56">56</option>
                            <option value="63">63</option>
                            <option value="70">70</option>
                            <option value="77">77</option>
                            <option value="84">84</option>
                            <option value="91">91</option>
                        </select>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.MOQValue.$invalid && FormContractInfo.MOQValue.$touched}">
                        <label class="fld-required">MOQ / Value</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="MOQValue" ng-paste="paste($event)" ng-keypress="filterValue($event)"  ng-disabled="disable" ng-model="ContractInfo.MOQValue" required> 
                            <p ng-show="FormContractInfo.MOQValue.$invalid && FormContractInfo.MOQValue.$touched" class="help-block">This field is required.</p> 
                        </div>
                    </div>
                </div>
            
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.PickUpTerms.$invalid && FormContractInfo.PickUpTerms.$touched}">
                        <label class="fld-required">Pick up Terms</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="PickUpTerms" ng-disabled="disable" ng-model="ContractInfo.PickUpTerms" required> 
                            <p ng-show="FormContractInfo.PickUpTerms.$invalid && FormContractInfo.PickUpTerms.$touched" class="help-block">This field is required.</p> 
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" >
                        <label class="fld-required">Commercial/Trade License No</label>
                        <div class="input-group">
                            <input type="text" class="form-control" ng-paste="paste($event)" name="TradeLicenseNo" ng-disabled="disable" ng-model="ContractInfo.TradeLicenseNo" ng-keypress="filterValue($event)" required> 
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
            <!-- </div>
            <div class="row"> -->
                 <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.TaxID.$invalid && FormContractInfo.TaxID.$touched}">
                        <label class="fld-required">VAT / GST / Tax ID</label>
                        <div class="input-group">
                            <input type="text" name="TaxID" ng-paste="paste($event)" class="form-control" id="VGTID" ng-disabled="disable" ng-model="ContractInfo.TaxID" ng-keypress="filterValue($event)" required> 
                            <p ng-show="FormContractInfo.TaxID.$invalid && FormContractInfo.TaxID.$touched" class="help-block">This field is required.</p> 
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Contract Renewal Reminder 1 </label>
                            <div class="input-group">
                                <input type="text" class="form-control" name="RenewalReminder1Date" id="RenewalReminder1Date" ng-disabled="disable" ng-model="ContractInfo.RenewalReminder1Date" ng-init="InputMask()">
                                <span class="input-group-addon" id="RenewalReminder1Date" >
                                    <span class="fa fa-calendar" id="reminder1"></span>
                                </span>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Contract Renewal Reminder 2</label>
                            <div class="input-group">
                                <input type="text" class="form-control" name="RenewalReminder2Date" id="RenewalReminder2Date" ng-disabled="disable" ng-model="ContractInfo.RenewalReminder2Date" ng-init="InputMask()">
                                <span class="input-group-addon" id="RenewalReminder2Date" >
                                    <span class="fa fa-calendar" id="reminder2"></span>
                                </span>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Contract Renewal Reminder 3 </label>  
                            <div class="input-group">
                                <input type="text" class="form-control" name="RenewalReminder3Date" id="RenewalReminder3Date" ng-disabled="disable" ng-model="ContractInfo.RenewalReminder3Date" ng-init="InputMask()">
                                <span class="input-group-addon" id="RenewalReminder3Date" >
                                    <span class="fa fa-calendar" id="reminder3"></span>
                                </span>
                        </div>
                    </div>
                </div>
            <!-- </div>
            <div class="row"> -->
                <div class="col-xs-12 col-sm-6 col-md-3" ng-if="ContractInfo.Advance == 1">
                    <div class="form-group" ng-class="{'has-error' : FormContractInfo.AdvanceDeposit.$invalid && FormContractInfo.AdvanceDeposit.$touched}">
                        <label class="fld-required">Advance Deposit</label>
                        <div class="input-group">
                            <input type="text" class="form-control" id="AdvanceDeposit" ng-disabled="disable" ng-model="ContractInfo.AdvanceDeposit" ng-keypress="filterValue($event)" ng-paste="paste($event)" required> 
                            <p ng-show="FormContractInfo.AdvanceDeposit.$invalid && FormContractInfo.AdvanceDeposit.$touched" class="help-block">This field is required.</p>
                        </div>
                    </div>
                </div> 
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Contract Upload Option</label>
                        <div class="input-group file-group" id="contract-upload-ctrl" ng-init="initContractUpload()">
                            <div>
                                <input type="file" class="file-pick" name="ContractUpload" accept="image/x-png,image/jpeg"/>
                            </div>
                            <input type="text" class="form-control" readonly/>
                            <div class="file-controls">
                                <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Upload Trade License </label>
                        <div class="input-group file-group" id="trade-license-ctrl" ng-init="initTradeLicense()">
                            <div>
                                <input type="file" class="file-pick" name="TradeLicense" accept="image/x-png,image/jpeg"/>
                            </div>
                            <input type="text" class="form-control" readonly/>
                            <div class="file-controls">
                                <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Upload Tax Certificate</label>
                        <div class="input-group file-group" id="tax-certificate-ctrl" ng-init="initTaxCertificate()">
                            <div>
                                <input type="file" class="file-pick" name="TaxCertificate" accept="image/x-png,image/jpeg"/>
                            </div>
                            <input type="text" class="form-control" readonly/>
                            <div class="file-controls">
                                <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12">
                    <div class="form-group">
                        <div class="row mt-10">
                            <div class="col-xs-10">
                                <div ng-if="CIDShowErrorMsg" class="alert alert-danger">
                                    <button class="close" data-close="alert"></button>
                                    <span class="error-txt">{{CIDErrorMsg}}</span>
                                </div>
                            </div>
                            <label>&nbsp;</label>
                            <div class="action-group">
                                <button type="button" ng-click="saveContractInfo(FormContractInfo.$valid)" class="btn blue fa fa-save" ng-disabled="FormContractInfo.$invalid"> Save</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<div class="modal fade in" id="ContractInfo" data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h3><span class="modal-title custom-font-color">RNT - Linen Supplier Management</span></h3>
    </div>
    <div class="modal-header">
        <p><h3>Contract Info - Saved Successfully</h3></p>
        <button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
    </div>
</div>