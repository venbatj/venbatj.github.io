.page-sidebar .page-sidebar-menu-container > ul ul li > ul,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > ul {
    margin: 0;
}
.page-sidebar .page-sidebar-menu-container > ul ul li > ul > li,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > ul > li {
    /* 3rd level sub menu */
}
.page-sidebar .page-sidebar-menu-container > ul ul li > ul > li > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > ul > li > a {
    padding-left: 40px;
