<form name="LaundryAssignment" class="horizontal-form" method="post">
    <div class="portlet-body">
        <div class="form-body">
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Select Laundry Vendor</label>
                        <div class="input-group">
                            <select id="laundry-vendor" name="LaundryVendor" class="form-control select2" ng-model="LaundryAssignment.LaundryVendor" ng-change="onLaundryVendorChange(Lv.LaundryVendor)"> 
                                <option value="{{Lv.ID}}" ng-repeat="Lv in LaundryVendorList">{{Lv.LaundryVendor}}</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Select Customer</label>
                        <div class="input-group">
                            <select id="customer" name="Customer" class="form-control select2" ng-model="LaundryAssignment.Customer" ng-change="onCustomerChange(C.Customer)"> 
                                <option value="{{C.ID}}" ng-repeat="C in CustomerList">{{C.Customer}}</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>Start Date</label>
                        <div class="input-group">
                            <input type="text" class="form-control" ng-model="LaundryAssignment.StartDate" name="StartDate">
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <div class="form-group">
                        <label>End Date</label>
                        <div class="input-group">
                            <input type="text" class="form-control" ng-model="LaundryAssignment.EndDate" name="EndDate">
                        </div>
                    </div>
                </div>
                <div class="portlet light mb-3">
                    <div class="portlet-body">
                        <div class="table-responsive">
                            <table class="table table-bordered" id="price-calculator-table">
                                <thead>
                                    <tr>
                                        <th>Item</th>
                                        <th>Qty</th>
                                        <th>Std Costing</th>
                                        <th>Customer Quote</th>
                                        <th>Combined Costing</th>
                                        <th>Edit</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>