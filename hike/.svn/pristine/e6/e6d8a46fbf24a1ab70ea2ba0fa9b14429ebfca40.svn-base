<div class="portlet-body">
	<div class="form-body">
		<div class="row">
			<form name="FormContactInfo" id="form-contact-info" role="form" novalidate>
				<div class="form-body">
					<div class="row">
						<div class="col-xs-12 col-sm-6 col-md-3">
							<div class="form-group" ng-class="{'has-error' : FormContactInfo.FirstName.$invalid && FormContactInfo.FirstName.$touched}">
								<label class="fld-required">First Name</label>
								<div class="input-group">
									<input type="text" ng-disabled="disable" ng-model="ContactInfo.FirstName" name="FirstName" onkeydown="return /^[a-zA-Z\. ]*$/i.test(event.key)"class="form-control" required /> 
									<p ng-show="FormContactInfo.FirstName.$invalid && FormContactInfo.FirstName.$touched" class="help-block">This field is required.</p>
								</div>
							</div>
						</div>
						<div class="col-xs-12 col-sm-6 col-md-3">
							<div class="form-group">
								<label>Last Name</label>
								<div class="input-group">
									<input type="text" ng-disabled="disable" ng-model="ContactInfo.LastName" name="LastName" onkeydown="return /^[a-zA-Z\. ]*$/i.test(event.key)"class="form-control" /> 
								</div>
							</div>
						</div>
						<div class="col-xs-12 col-sm-6 col-md-3">
							<div class="form-group" ng-class="{'has-error' : FormContactInfo.Phone.$invalid && FormContactInfo.Phone.$touched}">
								<label class="fld-required">Phone #</label>
								<div class="input-group">
									<input type="text" ng-disabled="disable" ng-model="ContactInfo.Phone" ng-paste="paste($event)" ng-keypress="filterValue($event)"  name="Phone" class="form-control" required /> 
									<p ng-show="FormContactInfo.Phone.$invalid && FormContactInfo.Phone.$touched" class="help-block">This field is required.</p>
								</div>
							</div>
						</div>
						<div class="col-xs-12 col-sm-6 col-md-3">
							<div class="form-group" ng-class="{ 'has-error' : FormContactInfo.EmailID.$dirty && FormContactInfo.EmailID.$error.pattern}">
								<label class="fld-required">Email</label>
								<div class="input-group">
									<input type="text" class="form-control" name="EmailID" ng-disabled="disable" ng-model="ContactInfo.EmailID" 
									ng-pattern="/^[^\s@]+@[^\s@]+\.[^\s@]{2,}$/">
									<p ng-show="FormContactInfo.EmailID.$dirty && FormContactInfo.EmailID.$error.pattern" 
									class="help-block">Please Enter Valid Email</p> 
								</div>
							</div>
						</div>						
					</div>
					<div class="row">
						<div class="col-xs-12 col-md-4">
							<div class="form-group" ng-if="!HideInviteContact">
								<label>{{LCSendInviteLabel}}</label>
								<div class="mt-radio-inline">
									<label class="mt-radio">
										<input type="radio" name="SendInvite" id="LCSendInviteY" ng-disabled="disable" ng-model="ContactInfo.SendInvite" value="Y"> Yes
										<span></span>
									</label>
									<label class="mt-radio">
										<input type="radio" name="SendInvite" id="LCSendInviteN" ng-disabled="disable" ng-model="ContactInfo.SendInvite" value="N"> No
										<span></span>
									</label>
								</div>
							</div>
						</div> 
						<div class="col-xs-12 col-md-8">
							<div class="form-group">
								<label>&nbsp;</label>
								<div class="action-group">
									<button type="button" ng-click="saveLinenContact(FormContactInfo)" class="btn blue fa fa-save" ng-disabled="FormContactInfo.$invalid"> Save</button>
									<button type="button" ng-click="clearLinenContact(FormContactInfo)" class="btn green fa fa-close"> Clear</button>
								</div>
							</div>
						</div>
						<div class="col-xs-12">
							<div ng-if="LIShowErrorMsg" class="alert alert-danger">
								<button class="close" data-close="alert"></button>
								<span class="error-txt">{{LIErrorMsg}}</span>
							</div>
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>
<div class="portlet-body">
	<div class="form-body">
		<div class="portlet light form-view mb-3" >
			<div class="portlet-title">
				<div class="caption">
					<h3 class="UserName">
						Contact Info
					</h3>
				</div>					
			</div>
			<div class="row">
				<div class="col-sm-12">
					<table id="linen-contact-data-table" class="table table-striped table-bordered table-hover dt-responsive" width="100%">
						<thead>
							<tr>
								<td class="check-box-column">
									<label class="mt-checkbox mt-checkbox-single mt-checkbox-outline" for="check-all-linen-contact">
										<input type="checkbox" class="checkboxes check-all" value="1" id="check-all-linen-contact"/>
										<span></span>
									</label>
								</td>
								<td>Name</td>
								<td>Email</td>
								<td>Phone</td>
								<td class="action-column">Edit</td>
							</tr>
						</thead>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="modal fade in" id="contactInfo" data-width="450">  
	<div class="modal-header" style="background-color: #299d9d;">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
		<h3><span class="modal-title custom-font-color">RNT - Linen supplier Management</span></h3>
	</div>
	<div class="modal-header">
		<p><h3>Cantact Info - Saved Successfully</h3></p>
		<button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
	</div>
</div>