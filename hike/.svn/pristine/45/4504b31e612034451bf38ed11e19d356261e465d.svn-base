.page-sidebar .page-sidebar-menu-container > ul ul li > ul > li > ul > li > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > ul > li > ul > li > a {
