<div class="ui-page compact">
    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="top-control-bar">
                        <h3 style="text-align: left;"><strong>Prospect </strong></h3>
                        <div style="text-align: right;" class="action-group">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="table-responsive">
                <table class="table table-bordered table-hover dt-responsive" width="100%" id="prospect-data-table">
                    <thead>
                        <tr>
                            <th ng-model="FormData.CustomerName">Prospect</th>
                            <th ng-model="FormData.HOCityID">City</th>
                            <th ng-model="FormData.Modified">Date</th>
                            <th ng-model="FormData.BOM">Bill Of Material</th>
                            <th ng-model="FormData.Action">Action</th>
                            <th ng-model="FormData.FileName">Attachment</th>
                        </tr>
                    </thead>
                </table>
            </div>	
        </div>
    </div>
</div>
<div class="modal fade in" id="Billofmaterial"  data-width="1200">  
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
       <!-- <i class="fa  fa-dollar (alias)"></i> -->
       <h4><span class="modal-title">Bill Of Material</span></h4>
    </div>
    <div class="table-responsive">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th> Item </th>
                    <th> Qty </th>
                    <th> Cost of Linen </th>
                    <th> Purchase Cost </th>
                    <th> Linen Cost / use </th>
                    <th> Tech Cost / Use </th>
                    <th> Delivery Cost/ Use </th>
                    <th> Warehouse Cost </th>
                    <th> Laundry Cost </th>
                    <th> Total Cost </th>
                    <th> Price to Customer </th>
                    <th> Billable Qty </th>
                    <th> Est. billing / day </th>
                    <th> Breakeven Days </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="Item in FormData.QuoteItems"> {{Item}}
                    <td type="text" contenteditable='false' ng-model="QuoteItems.ProductVariant">{{Item.ProductName}} - {{Item.VariantName}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.Qty">{{Item.Qty}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.CostOfLinen">{{Item.CostOfLinen | currency : "AED " }}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.PurchaseCost">{{Item.PurchaseCost | currency : "AED " : 0}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.LinenCostPerUse">{{Item.LinenCostPerUse | currency : "AED " : 2}}</td>								
                    <td type="text" contenteditable='false' ng-model="QuoteItems.TechCostPerUse">{{Item.TechCostPerUse | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.DeliveryCostPerUse">{{Item.DeliveryCostPerUse | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.WarehouseCost">{{Item.WarehouseCost | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.LaundryCost">{{Item.LaundryCost | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.TotalCost">{{Item.TotalCost | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.PriceToCustomer">{{Item.PriceToCustomer | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.BillableQty">{{Item.BillableQty}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.EstBillingPerDay">{{Item.EstBillingPerDay | currency : "AED " : 2}}</td>
                    <td type="text" contenteditable='false' ng-model="QuoteItems.BreakevenDays">{{Item.BreakevenDays}}</td>
                    <td><i title="Remove" ng-click="removeItem($index)" class="delete-icon fa fa-trash font-red-thunderbird"></i></td>
                </tr>
            </tbody>
            <tfoot>
                <td colspan="2">Total</td>
                <td></td>
                <td>{{FormData.TotalPurchaseCost | currency : "AED " : 0}}</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tfoot>
        </table>
    </div>	
</div>