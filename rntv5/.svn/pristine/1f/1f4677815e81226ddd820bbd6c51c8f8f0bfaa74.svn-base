<div class="ui-page compact">
    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="top-control-bar">
                        <h3 style="text-align: left;">
							<button type="button" ng-click="backToManagmentPage()" class="btn blue"><i class="fa fa-reply-all"> Back</i></button>
                            <strong>Import Customer</strong></h3>
                        <div style="text-align: right;" class="action-group">
                            <button type="button" ng-init="importInit()" ng-click="selectFile()" class="btn green">
                                    <i class="fa fa-arrow-circle-down"> Import</i></button>
                            <!-- <input id="import-customer-file" type="file" class="hide" accept="application/vnd.ms-excel"/> -->
                            <a target="_blank" class="btn blue" href="../data-storage/templates/CustomerImport.xls">
                                <i class="fa fa-download"> Download Template</i></a>

                            <div class="input-group file-group hide" id="import-customer-ctrl" ng-init="initTradeLicense()">
                                <div>
                                    <input type="file" class="file-pick" name="TradeLicense" accept=".xls"/>
                                </div>
                                <input type="text" class="form-control" readonly/>
                                <div class="file-controls">
                                    <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                    <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                    <i title="Remove" class="fa fa-trash font-red-mint rm-btn"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="portlet light">
        <div class="portlet-body">
            <table id="import-customer-data-table" class="table table-striped table-bordered table-hover dt-responsive" width="100%">
                <thead>
                    <tr>
                        <td>Imported At</td>
                        <td>Source File</td>
                        <td class="action-column">Report</td>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>  