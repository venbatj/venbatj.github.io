<form name="FormCompareLaundry">
    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <h3>
                        <button ng-click="BackManagement()" class="btn blue fa fa-reply-all"> Back</button>
                        <strong>Warehouse Assign Pick up / Delivery </strong>
                    </h3>
                </div>
            </div>
        </div>
    </div>
    <div class="row custom-row">
        <div class="portlet light mb-3">
            <div class="portlet-body">
                <div class="portlet-body">
                    <div class="form-body">
                        <div class="row">
                            <div  class="col-xs-12 col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label>Warehouse Name</label>
                                    <div class="input-group">
                                        <select id="warehouse-delivery" name="CustomerInventory" class="form-control select2"
                                        ng-model="ScheduleData.WarehouseName" ng-change="onCustInventoryChange(ScheduleData.WarehouseName)"> 
                                        <option value="{{W.ID}}" ng-repeat="W in Warehouselist">{{W.WarehouseName}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div> 
                            <div  class="col-xs-12 col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label>Deliver To</label>
                                    <div class="input-group">
                                        <select id="deliver" name="Deliver" class="form-control select2" ng-model="ScheduleData.Deliver"> 
                                        <option value="{{CL.ID}}" ng-repeat="CL in DeliverList">{{CL.Deliver}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.Deliver==1">
                                <div class="form-group">
                                    <label>Laundry</label>
                                    <div class="input-group">
                                        <select id="schedule-laundry" name="LaundryID" class="form-control select2"
                                        ng-model="ScheduleData.LaundryID" ng-change="onLaundryChange(ScheduleData.LaundryID)"> 
                                        <option value="{{L.ID}}" ng-repeat="L in LaundryList">{{L.LaundryName}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.Deliver==1">
                                <div class="form-group">
                                    <label>Laundry Location</label>
                                    <div class="input-group">
                                        <select id="assign-laundry-location" name="LaundryLocationID" class="form-control select2"
                                        ng-model="ScheduleData.LaundryLocationID"ng-change="onLocationChange(ScheduleData.LaundryLocationID)" required> 
                                            <option value="{{LL.ID}}" ng-repeat="LL in LaundryLocationList">{{LL.LocationName}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.Deliver==2">
                                <div class="form-group">
                                    <label>Customer</label>
                                    <div class="input-group">
                                        <select id="customer-delivery" name="CustomerInventory" class="form-control select2"
                                        ng-model="ScheduleData.CustomerName" ng-change="onCustomerChange(ScheduleData.CustomerName)"> 
                                        <option value="{{CD.ID}}" ng-repeat="CD in CustomerDeliveryList">{{CD.CustomerName}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.Deliver==2">
                                <div class="form-group">
                                    <label>Customer Location</label>
                                    <div class="input-group">
                                        <select id="customer-location" name="LocationName" class="form-control select2"
                                        ng-model="ScheduleData.LocationName" ng-change="onLocationChange(ScheduleData.LocationName)"> 
                                        <option value="{{CL.ID}}" ng-repeat="CL in DeliveryLocationList">{{CL.LocationName}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label>Type</label>
                                    <div class="input-group">
                                        <select id="deliver-type" name="Type" class="form-control select2" ng-model="ScheduleData.Type"> 
                                        <option value="{{CL.ID}}" ng-repeat="CL in TypeList">{{CL.Type}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label>Delivery time & Time</label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" id="sdeliverytime" ng-model="ScheduleData.DeliveryDateTime"> 
                                        <span class="input-group-addon" id="sdelivery-time" >
                                            <span class="fa fa-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3">
                                <div class="form-group">
                                    <label>Send request to</label>
                                    <div class="input-group">
                                        <select id="request-type" name="SendRequest" class="form-control select2" ng-model="ScheduleData.SendRequest"> 
                                        <option value="{{CL.ID}}" ng-repeat="CL in RequestList">{{CL.SendRequest}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.SendRequest==1">
                                <div class="form-group">
                                    <label>RNT Delivery Incharge</label>
                                    <div class="input-group">
                                        <input type="text" id="laundry-data" ng-model="ScheduleData.DeliveryIncharge" class="form-control" name="DeliveryIncharge">
                                    </div>
                                </div>
                            </div>
                            <div  class="col-xs-12 col-sm-6 col-md-3" ng-if="ScheduleData.SendRequest==2">
                                <div class="form-group">
                                    <label>Laundry Incharge</label>
                                    <div class="input-group">
                                        <input type="text" id="laundry-data" ng-model="ScheduleData.LaundryIncharge" class="form-control" name="LaundryIncharge">
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <div class="action-group">
                                        <button type="button" ng-click="assignDeliverydata(FormCompareLaundry)" class="btn blue fa fa-save"> Save</button>
                                        <button type="button" ng-click="ClearDleivery(FormCompareLaundry)" class="btn blue fa fa-close"> Cancel</button>
                                    </div>
                                </div>
                            </div>  
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class ="row" ng-if="DeliveryData.OrderTyping ==2">
            <form name="FrmDelivery">
                <div class="col-md-12">
                    <div class="portlet light mb-3">
                        <div class="form-body">
                            <div class ="row">
                                <div class="col-xs-12 col-sm-6 col-md-3">
                                    <div class="form-group">
                                        <label>Category</label>
                                        <div class="input-group">
                                            <select id="category" name="Category" class="form-control select2"
                                            ng-model="SubDeliveryData.CategoryID" ng-change="onCategoryChange(SubDeliveryData.CategoryID)" > 
                                                <option value="{{Category.ID}}" ng-repeat="Category in CategoryList">{{Category.CategoryName}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-3">
                                    <div class="form-group">
                                        <label>Product</label>
                                        <div class="input-group">
                                            <select id="product" name="Product" class="form-control select2"
                                            ng-model="SubDeliveryData.ProductID" ng-change="onProductChange(SubDeliveryData.ProductID)"  > 
                                                <option value="{{Product.ID}}" ng-repeat="Product in ProductList">{{Product.ProductName}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-3">
                                    <div class="form-group">
                                        <label>Variant</label>
                                        <div class="input-group">
                                            <select id="variant" name="Variant" class="form-control select2"
                                            ng-model="SubDeliveryData.VariantID" ng-change="onVariantChange(SubDeliveryData.VariantID)"> 
                                                <option value="{{Variant.VariantID}}" ng-repeat="Variant in VariantList">{{Variant.VariantName}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-3">
                                    <div class="form-group">
                                        <label>Required Qty</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="Qty" ng-model="SubDeliveryData.Qty"> 
                                        </div>
                                    </div>
                                </div>
                                <!-- <div class="col-xs-12">
                                    <div class="form-group">
                                        <label>&nbsp;</label>
                                        <div class="action-group">
                                            <button type="button" ng-click="DeliveryAssign()" class="btn blue fa fa-tasks menu-icon" ng-disabled="FrmDelivery.$invalid">
                                            Assign Delivery</button>
                                        </div>
                                    </div>
                                </div> -->
                            </div>					
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <div class="portlet light mb-3" ng-if="DeliveryData.OrderTyping ==2">
            <div class="portlet-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="manage-pickup-delivery">
                        <thead>
                            <tr>
                                <th> Category </th>
                                <th> Product </th>
                                <th> Variant </th>
                                <th> Qty </th>
                            </tr>
                        </thead> 
                        <tbody>
                            <tr ng-repeat="Data in DeliveryDataPickup" ng-show="childdelivery"> 
                                <td>{{Data.CategoryName}}</td>
                                <td>{{Data.ProductName}}</td>
                                <td>{{Data.VariantName}}</td>
                                <td>{{Data.Qty}}</td>
                            </tr>
                        </tbody>
                    </table>
                    <!-- <div class="col-xs-12">
                        <div class="form-group">
                            <label>&nbsp;</label>
                            <div class="action-group">
                                <button type="button" ng-click="assignDelivery(FrmDelivery)" class="btn blue fa fa-tasks menu-icon"> Assign</button>
                                <button type="button" ng-click="ClearDleivery(FrmDelivery)" class="btn blue fa fa-close"> Cancel</button>
                            </div>
                        </div>
                    </div>   -->
                </div>	
            </div>
        </div>
    </div>
</form>
<div class="portlet-body">
    <div class="form-body">
        <div class="portlet light form-view mb-3" >
            <div class="portlet-title">
                <div class="caption">
                    <h3 class="UserName">
                        Scheduled Pickup
                    </h3>
                </div>					
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <table id="schedule-manage-data-table" class="table table-striped table-bordered table-hover dt-responsive" width="100%">
                        <thead>
                            <tr>
                                <td class="check-box-column">
                                    <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline" for="check-all-delivery-pickup">
                                        <input type="checkbox" class="checkboxes check-all" value="1" ng-model="selectAll" id="check-all-delivery-pickup"/>
                                        <span></span>
                                    </label>
                                </td>
                                <td>Warehouse Name</td>
                                <td>Customer Name</td>
                                <td>Customer Location</td>
                                <td>Laundry Name</td>
                                <td>Laundry Location Name</td>
                                <td>Scheduled Delivery Time</td>
                                <td class="action-column">Edit</td>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>	