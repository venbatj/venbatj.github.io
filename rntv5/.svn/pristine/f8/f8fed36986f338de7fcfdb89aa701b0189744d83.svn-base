<div class="modal fade in" id="Changepassword"  data-width="500">  
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h4><i class="fa fa-unlock-alt"></i>
        <span class="modal-title">Change Password</span></h4>
    </div>
    <div class="modal-body">
        <div class="flip-scroll">
            <form name= "FrmChangePassword" role="form" class="form-horizontal" novalidate>
                <div class="row" id="data">
                    <div class="col-md-12">
                        <script>
                            function mouseoverPass(obj) {
                                var obj = document.getElementById('crtpass');
                                obj.type = "text";
                            }
                            function mouseoutPass(obj) {
                                var obj = document.getElementById('crtpass');
                                obj.type = "password";
                            }
                        </script>
                        <div class="form-group" ng-class="{ 'has-error' : FrmChangePassword.CurrentPassword.$invalid &&  FrmChangePassword.CurrentPassword.$touched}">
                            <label class=" col-md-4 control-label lbl-font fld-required fld-seperator">Current Password</label>
                            <div class=" col-md-6">
                                <input type="password" id="crtpass" class="form-control" name="CurrentPassword" ng-model="FormData.CurrentPassword" required>
                                <i  class="fa fa-lg fa-eye field-icon" onmouseover="mouseoverPass();" onmouseout="mouseoutPass();"></i> 
                                <p ng-show="FrmChangePassword.CurrentPassword.$invalid && FrmChangePassword.CurrentPassword.$touched" class="help-block">This field is required.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group" ng-class="{ 'has-error' : FrmChangePassword.NewPassword.$invalid &&  FrmChangePassword.NewPassword.$touched}">
                            <label class=" col-md-4 control-label lbl-font fld-required fld-seperator">New Password</label>
                            <div class=" col-md-6">
                                <input type="password" id="NewPassword" class="form-control" name="NewPassword" ng-model="FormData.NewPassword" required>
                                <p ng-show="FrmChangePassword.NewPassword.$invalid && FrmChangePassword.NewPassword.$touched" class="help-block">This field is required.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <br>
                <div class="row">
                    <div class="col-md-7">
                        <div class="button-bar pull-right">
                            <button type="submit" class="btn btn-outline green default" ng-click="FormSubmit(FrmChangePassword.$valid)" ng-disabled="FrmChangePassword.$invalid" >Submit</button>
                            <button type="button" ng-click="FormReset()" class="btn btn-outline red" >Cancel</button>
                        </div>
                    </div>
                </div>
                <div ng-if="ShowErrorMsg" class="alert alert-danger">
                    <button class="close" data-close="alert"></button>
                    <span class="error-txt">{{ErrorMsg}}</span>
                </div>
            </form>
        </div>
    </div>
</div>