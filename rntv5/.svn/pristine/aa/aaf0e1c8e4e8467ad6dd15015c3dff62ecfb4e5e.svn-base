<div class="ui-page compact">
    <div class="portlet light mb-3">
        <div class="portlet-body">
            <div class="row">
                <div class="col-md-12">
                    <h3><strong>Collaterals</strong></h3>
                </div>
            </div>
        </div>
    </div>
    <form name="FormCollaterals" class="horizontal-form" method="post">
        <div class="portlet light mb-3">
            <div class="portlet-body">
                <div class="form-body">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-md-3">
                            <div class="form-group">
                                <label class="fld-required"> Contract Template</label>
                                <div class="input-group file-group" id="upload-contract-temp" ng-init="initContractTemplate()">
                                    <div>
                                        <input type="file" class="file-pick" name="ContractTemplate" accept="image/x-png,image/jpeg"/>
                                    </div>
                                    <input type="text" class="form-control" readonly/>
                                    <div class="file-controls">
                                        <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                        <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                        <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3">
                            <div class="form-group">
                                <label>Terms & Condition template</label>
                                <div class="input-group file-group" id="upload" ng-init="initTerms&ConditionTemp()">
                                    <div>
                                        <input type="file" class="file-pick" name="TaxCertificate" accept="image/x-png,image/jpeg"/>
                                    </div>
                                    <input type="text" class="form-control" readonly/>
                                    <div class="file-controls">
                                        <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                        <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                        <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-3">
                            <div class="form-group">
                                <label>Sales ppt </label>
                                <div class="input-group file-group" id="upload" ng-init="initSalesppt()">
                                    <div>
                                        <input type="file" class="file-pick" name="TaxCertificate" accept="image/x-png,image/jpeg"/>
                                    </div>
                                    <input type="text" class="form-control" readonly/>
                                    <div class="file-controls">
                                        <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                        <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                        <i title="Remove" class="fa fa-trash font-red-mint rm-btn" ng-click="removeLogo()"></i>
                                    </div>
                                </div>
                            </div>
                        </div>     
                        <div class="col-xs-12 col-sm-6 col-md-3">
                            <div class="form-group">
                                <label>Others</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" ng-model="Collaterals.State"> 
                                </div>
                            </div>
                        </div>  
                        <div style="text-align: right;" class="action-group">
                            <button type="button" ng-click="saveLinenDetail(FormCollaterals.$valid)" class="btn blue" ng-disabled="FormCollaterals.$invalid">
                                <i class="fa fa-save"> Save</i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>