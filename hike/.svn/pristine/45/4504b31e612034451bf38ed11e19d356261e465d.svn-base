.page-sidebar .page-sidebar-menu-container > ul ul li > a .arrow:before,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > a .arrow:before {
    margin-top: -4px;
