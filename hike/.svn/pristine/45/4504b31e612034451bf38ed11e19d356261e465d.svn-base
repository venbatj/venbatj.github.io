@charset "UTF-8";
/* Cubic Bezier Transition */


.page-sidebar .page-sidebar-menu-container > ul,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul {
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    border-radius: 4px;
    list-style: none;
    margin: 30px 0 0 0;
    padding: 0;
    /* 1st level links */
    /* all links */
}
.page-sidebar .page-sidebar-menu-container > ul > li,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li {
    display: block;
    margin: 0;
    padding: 0;
    border: 0px;
}
.page-sidebar .page-sidebar-menu-container > ul > li.heading,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li.heading {
    height: 50px;
    padding: 25px 15px 10px 15px;
}
.page-sidebar .page-sidebar-menu-container > ul > li.heading > h3,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li.heading > h3 {
    font-size: 13px;
    font-weight: 400;
    padding: 0;
    margin: 0;
    font-weight: 600;
}
.page-sidebar .page-sidebar-menu-container > ul > li > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a {
    display: block;
    position: relative;
    margin: 1px 0 0 0;
    border: 0px;
    padding: 10px 15px;
    text-decoration: none;
}
.page-sidebar .page-sidebar-menu-container > ul > li > a > .title,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a > .title {
    font-size: 16px;
    font-weight: 500;
}
.page-sidebar .page-sidebar-menu-container > ul > li > a > i,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a > i {
    font-size: 18px;
    text-shadow: none;
    font-weight: 300;
}
.page-sidebar .page-sidebar-menu-container > ul > li > a > i.glyphicon,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a > i.glyphicon {
    top: 3px;
    margin-left: 1px;
    margin-right: 4px;
}
.page-sidebar .page-sidebar-menu-container > ul > li > a > [class^="icon-"],
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a > [class^="icon-"] {
    top: 2px;
    margin-left: 1px;
    margin-right: 4px;
}
.page-sidebar-fixed .page-sidebar .page-sidebar-menu-container > ul > li > a,
.page-sidebar-fixed .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a {
    transition: all 0.2s ease;
}
.page-sidebar-reversed.page-sidebar-fixed .page-sidebar .page-sidebar-menu-container > ul > li > a,
.page-sidebar-reversed.page-sidebar-fixed .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li > a {
    transition: none;
}
.page-sidebar .page-sidebar-menu-container > ul > li.open > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li.open > a {
    font-size: 14px;
}
.page-sidebar .page-sidebar-menu-container > ul > li.active > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul > li.active > a {
    border: none;
    text-shadow: none;
    font-size: 14px;
}
.page-sidebar .page-sidebar-menu-container > ul li > a > .arrow:before,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul li > a > .arrow:before {
    width: 10px;
    float: right;
    margin-right: 5px;
    margin-left: 5px;
    display: inline;
    font-size: 16px;
    font-family: FontAwesome;
    height: auto;
    content: "\f104";
    font-weight: 300;
    text-shadow: none;
}
.page-sidebar .page-sidebar-menu-container > ul li > a > .arrow.open:before,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul li > a > .arrow.open:before {
    content: "\f107";
    margin-right: 6px;
}
.page-sidebar .page-sidebar-menu-container > ul li > a > .badge,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul li > a > .badge {
    float: right;
    margin-top: 1px;
    margin-right: 0px;
}
.page-sidebar .page-sidebar-menu-container > ul ul,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul {
    list-style: none;
    display: none;
    padding: 0;
    margin: 8px 0px 8px 0px;
}
.page-sidebar .page-sidebar-menu-container > ul ul li,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li {
    background: none;
    margin: 0px;
    padding: 0px;
    margin-top: 1px !important;
    /* 2nd level sub menu */
}
.page-sidebar .page-sidebar-menu-container > ul ul li > a,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > a {
    display: block;
    margin: 0;
    padding: 9px 14px 9px 30px;
    text-decoration: none;
    font-size: 15px;
    font-weight: 500;
    background: none;
}
.page-sidebar .page-sidebar-menu-container > ul ul li > a > i,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > a > i {
    font-size: 16px;
