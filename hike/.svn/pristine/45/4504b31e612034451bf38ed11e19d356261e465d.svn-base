.page-sidebar .page-sidebar-menu-container > ul ul li > ul > li > ul,
.page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu-container > ul ul li > ul > li > ul {
    margin: 0;
