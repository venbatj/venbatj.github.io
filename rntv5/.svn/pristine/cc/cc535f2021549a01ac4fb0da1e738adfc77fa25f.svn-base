<div class="ui-page">
    <form name="FrmCustomer" role="form" novalidate>
        <div class="portlet light mb-3">
            <div class="portlet-body">
                <div class="row" >
                    <div class="col-md-12">
                        <h3><strong>Laundry Vendor Management</strong></h3>
                    </div>
                </div>
            </div>
        </div>
        <div class="portlet light mb-3" id="customer-index">
            <div class="portlet-body">
                <div class="form-body">
                    <div class="row" id="rowinventory">
                        <div class="col-md-4 newLinen" id="prospecty">
                            <h4 id="head"><b>Create a New Laundry Vendor</b></h4><br>
                            <center><label ng-click="ViewLaundryPage()"><b><a><u id="linenlink">Click here to Add a New Laundry Vendor</u></a></b> 
                            <i class="fa fa-user-plus fa-2x plus-cust"  id="linenlink"></i>
                            </label></center>
                        </div>
                        <div class="col-md-4 newLinen" id="newcust">
                            <h4 id="head"><b> View/ Edit Laundry Vendor </b></h4>
                            <div class="col-xs-12">
                                <label><b></b></label>
                                <div class="input-group">
                                    <select id="laundry-vendor" name="LaundryVendorName" class="form-control select2" required
                                    ng-model="LaundryDetail.LaundryVendorName" ng-change="LaundryVendorNameChange(LaundryDetail.LaundryVendorName)"> 
                                        <option value="{{Lv.ID}}" ng-repeat="Lv in LaundryVendorList">{{Lv.LaundryName}}</option>
                                    </select>
                                </div>
                            </div>  
                        </div>
                        <div class="col-md-4 newLinen" id="exist">
                            <h4 id="head"><b>Comparisons Of Laundry Vendors</b></h4><br>
                            <div class ="row">
                                <form name="FrmComparison">
                                    <div class="col-xs-12 col-sm-4">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <select id="category" name="Category" class="form-control select2"
                                                ng-model="ComparisonLaundry.CategoryID" ng-change="onCategoryChange(ComparisonLaundry.CategoryID)" > 
                                                    <option value="{{Category.ID}}" ng-repeat="Category in CategoryList">{{Category.CategoryName}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-4">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <select id="product" name="Product" class="form-control select2"
                                                ng-model="ComparisonLaundry.ProductID" ng-change="onProductChange(ComparisonLaundry.ProductID)"  > 
                                                    <option value="{{Product.ID}}" ng-repeat="Product in ProductList">{{Product.ProductName}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-4">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <select id="variant" name="Variant" class="form-control select2"
                                                ng-model="ComparisonLaundry.VariantID" ng-change="onVariantChange(ComparisonLaundry.VariantID)"> 
                                                    <option value="{{Variant.VariantID}}" ng-repeat="Variant in VariantList">{{Variant.VariantName}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12">
                                        <div class="form-group">
                                            <div class="action-group">
                                                <center>
                                                <button type="button" ng-click="ComparisonData(ComparisonLaundry.CategoryID, ComparisonLaundry.ProductID, ComparisonLaundry.VariantID)" style="border:#3d3830;background-color:#3d3830;" ng-disabled="FrmComparison.$invalid">
                                                Go <i class="fa fa-hand-o-right"> </i></button></center>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>	
                            <!-- <center><label ng-click="CompareSuppliers()"><b><a><u id="linenlink">Click here to Comparisons Of Linen Suppliers</u></a></b> 
                            <i class="fa fa-balance-scale fa-2x plus-cust"  id="linenlink"></i>
                            </label></center> -->
                        </div>
                        <div class="col-md-4 newLinen">
                            <h4 id="head"><b>Import Laundry Vendors in Bulk</b></h4><br />
                            <a target="_blank" href="../data-storage/templates/LaundryImport.xls"><u id="linenlink"><b><center>Download Excel Template</center>
                            </b></u></a><br />
                            <a ng-init="importInit()" ng-click="selectFile()"> 
                                <u id="linenlink"><b><center>Upload Excel</center></b></u></a></p>
                                <a target="_blank" ><u id="linenlink"><b><center>Upload/Download Pricing Excel</center>
                                </b></u></a><br />
                            <div class="input-group file-group hide" id="import-laundry" ng-init="initTradeLicense()">
                                <div>
                                    <input type="file" class="file-pick" name="TradeLicense" accept=".xls"/>
                                </div>
                                <input type="text" class="form-control" readonly/>
                                <div class="file-controls">
                                    <i title="Select" class="fa fa-folder-open font-blue-ebonyclay up-btn"></i>
                                    <i title="Change" class="fa fa-edit font-green-jungle ch-btn"></i>
                                    <i title="Remove" class="fa fa-trash font-red-mint rm-btn"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 newLinen" id="excel">
                            <h4 id="head"><b>Assign Laundry to a Customer</b></h4><br />
                            <div class="col-xs-12">
                                <label><b></b></label>
                                <div class="input-group">
                                    <select id="asign-laundry" name="LaundryToCustomer" class="form-control select2" required
                                    ng-model="AssignCustomer.LaundryToCustomer" ng-change="onAssignLaundryChange(AssignCustomer.LaundryToCustomer)"> 
                                <option value="{{L.ID}}" ng-repeat="L in LaundryToCustomerList">{{L.LaundryToCustomer}}</option>
                                <!-- <option></option>
                                        <option>New Assignment</option>
                                        <option>Change</option>
                                        <option>Terminate</option> -->
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
<div class="modal fade in" id="compare-laundry" data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h4><span class="modal-title custom-font-color">RNT - Laundry Vendor</span></h4>
    </div>
    <div class="modal-header">
        <h4>Select Category, Product, Variant For Comparison Supplier</h4>
        <button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
    </div>
</div>
<div class="modal fade in" id="laundry-compare-category"  data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h4><span class="modal-title custom-font-color">RNT - Laundry Vendor</span></h4>
    </div>
    <div class="modal-header">
        <h4>Select a Category</h4>
        <button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
    </div>
</div>
<div class="modal fade in" id="laundry-compare-product"  data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h4><span class="modal-title custom-font-color">RNT - Laundry Vendor</span></h4>
    </div>
    <div class="modal-header">
        <h4>Select a Product</h4>
        <button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
    </div>
</div>
<div class="modal fade in" id="laundry-compare-variant"  data-width="450">  
    <div class="modal-header" style="background-color: #299d9d;">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="false"></button>
        <h4><span class="modal-title custom-font-color">RNT - Laundry Vendor</span></h4>
    </div>
    <div class="modal-header">
        <h4>Select a Variant</h4>
        <button class="btn green" style="float: right;" data-dismiss="modal" aria-hidden="false"> Ok</button>
    </div>
</div>