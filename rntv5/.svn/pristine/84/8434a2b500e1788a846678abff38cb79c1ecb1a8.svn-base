/* START DASHBOARD MENU FOR MEDGULF */
.DashboardMenu {
	width: 100%;
	position: relative;
	float: left;
	margin-bottom: 20px;
}

.DashboardMenu ul#HraMenu {
	width: 100%;
	list-style: none;
    margin: 0;
    padding: 0;
    direction: ltr !important;

    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;

}
.DashboardMenu ul#HraMenu li {
    width: 11.5%;
    position: relative;
    padding: 5px 0px;
    text-align: center;
}
.DashboardMenu ul#HraMenu li:FIRST-CHILD {
	width: 16.5%;
	text-align: right;
	padding-right: 20px;
	box-sizing: border-box;
}
.DashboardMenu ul#HraMenu li:nth-child(2),
.DashboardMenu ul#HraMenu li:nth-child(3) {
	width: 14%;
	text-align: center;
	box-sizing: border-box;
}
.DashboardMenu ul#HraMenu li:nth-last-child(-n+2) {
	width: 10.5%;
}

.DashboardMenu ul#HraMenu li:not(:nth-last-child(-n+2)):BEFORE {
	position: absolute;
	width: 100%;
	top: 21px;
	left: 0px;
    content: " ";
    height: 1px;
    border-top: 1px solid #ef4036;
    z-index: 0;
}

.DashboardMenu ul#HraMenu li:nth-last-child(2) {
	text-align: left;
}
.DashboardMenu ul#HraMenu li:last-child {
	text-align: right;
}

.DashboardMenu ul#HraMenu li a {
	display: inline-block;
    position: relative;
    z-index: 1;
    color: #ef4036;
    text-align: center;
}

.DashboardMenu ul#HraMenu li a::BEFORE,
.DashboardMenu ul#HraMenu li a::AFTER {
	content: '';
    position: absolute;
    right: auto;
    top: 13px;
    height: 7px;
    width: 7px;
    border-radius: 50%;
    -webkit-transition: background-color .3s,border-color .3s;
    -moz-transition: background-color .3s,border-color .3s;
    transition: background-color .3s,border-color .3s;

    background-color: #ef4036;
    border: 2px solid ;
}

.DashboardMenu ul#HraMenu li a::BEFORE {
	-webkit-transform: translateX(CALC(50% - 9px));
    -moz-transform: translateX(CALC(50% - 9px));
    -ms-transform: translateX(CALC(50% - 9px));
    -o-transform: translateX(CALC(50% - 9px));
    transform: translateX(CALC(50% - 9px));
}
.DashboardMenu ul#HraMenu li a::AFTER {
	-webkit-transform: translateX(CALC(50% + 9px));
    -moz-transform: translateX(CALC(50% + 9px));
    -ms-transform: translateX(CALC(50% + 9px));
    -o-transform: translateX(CALC(50% + 9px));
    transform: translateX(CALC(50% + 9px));
}
.DashboardMenu ul#HraMenu li a.Run-HRA::BEFORE,
.DashboardMenu ul#HraMenu li a.Run-HRA::AFTER,
.DashboardMenu ul#HraMenu li a.ViewTour::BEFORE,
.DashboardMenu ul#HraMenu li a.ViewTour::AFTER {
	display: none;
}
.DashboardMenu ul#HraMenu li:HOVER a::BEFORE,
.DashboardMenu ul#HraMenu li:HOVER a::AFTER {
	background-color: #017f9e;
}

.DashboardMenu ul#HraMenu li .title {
	font-size: 13px;
}

.DashboardMenu ul#HraMenu li a .progress-menu-icons {
	width: 30px;
	height: 30px;
	padding: 2px;
	
	-webkit-border-radius: 50% !important;
    -moz-border-radius: 50% !important;
    -ms-border-radius: 50% !important;
    -o-border-radius: 50% !important;
    border-radius: 50% !important;
    
    border: 0px solid #ef4036;
    background-color: #eeefef;
	
}
.DashboardMenu ul#HraMenu li a .hyjiya-icon {
	font-size: 20px;
    line-height: 20px;
}
.DashboardMenu ul#HraMenu li a .hyjiya-icon.progress-menu-icons {
	padding-top: 5px;
}
.DashboardMenu ul#HraMenu li.completed:BEFORE,
.DashboardMenu ul#HraMenu li.completed a,
.DashboardMenu ul#HraMenu li.completed a .progress-menu-icons {
	color: #73a81e;
	border-color: #73a81e;
}

.DashboardMenu ul#HraMenu li.active:BEFORE,
.DashboardMenu ul#HraMenu li.active a,
.DashboardMenu ul#HraMenu li.active a .progress-menu-icons,
.DashboardMenu ul#HraMenu li:HOVER:BEFORE,
.DashboardMenu ul#HraMenu li:HOVER a,
.DashboardMenu ul#HraMenu li:HOVER a .progress-menu-icons {
	color: #017f9e;
    border-color: #017f9e;
}
